{
  "framework": "sveltekit",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "outputDirectory": ".svelte-kit/vercel/output",
        "runtime": "nodejs18.x",
        "includeFiles": [
          "src/lib/contents/**",
          ".svelte-kit/vercel/output/**"
        ]
      }
    }
  ]
} 