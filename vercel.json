{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@sveltejs/adapter-vercel"
    },
    {
      "src": "contents/**/*.svx",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/contents/(.*)",
      "dest": "/contents/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
} 