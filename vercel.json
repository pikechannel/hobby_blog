{
  "framework": "sveltekit",
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "installCommand": "yarn",
  "outputDirectory": ".vercel/output",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "src/lib/contents/**",
          "articles/**"
        ]
      }
    }
  ]
} 