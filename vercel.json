{
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "framework": "sveltekit",
  "installCommand": "yarn install",
  "outputDirectory": ".svelte-kit/vercel/output",
  "routes": [
    {
      "src": "/contents/(.*)",
      "dest": "/contents/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/.svelte-kit/vercel/output/$1",
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/.svelte-kit/vercel/output/index.html"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/lib/contents/**"],
        "memory": 1024,
        "maxDuration": 10
      }
    }
  ]
} 