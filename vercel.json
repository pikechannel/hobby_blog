{
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "framework": "sveltekit",
  "installCommand": "yarn",
  "outputDirectory": ".svelte-kit/vercel/output",
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    {
      "src": "/contents/(.*)",
      "dest": "/contents/$1"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/lib/contents/**"]
      }
    }
  ]
} 