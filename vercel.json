{
  "framework": "sveltekit",
  "routes": [
    {
      "src": "/contents/(.*)",
      "dest": "/contents/$1"
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/lib/contents/**"],
        "memory": 1024,
        "maxDuration": 10
      }
    }
  ]
} 